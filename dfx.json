      "wasm": ""
    },
    "sns_index": {
      "build": "",
      "candid": "candid/sns_index.did",
      "type": "custom",
      "wasm": ""
    },
    "sns_ledger": {
      "build": "",
      "candid": "candid/sns_ledger.did",
      "type": "custom",
      "wasm": ""
    },
    "sns_rewards": {
      "build": ["./scripts/build-canister.sh sns_rewards"],
